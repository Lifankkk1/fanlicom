    <script type="text/javascript">
        function Init() {
            $("#mytab li").click(function (e) {
                e.preventDefault();
                $(this).tab("show");
            })
            $("#mytab1 li").mouseover(function (e) {
                e.preventDefault();
                $(this).tab("show");
            })
            $("#mytab1 li").mouseout(function () {
                $(this).removeClass("active");
            })
            //$(".breadcrumb").mouseover(function () {
            //    var i = $(".breadcrumb").length;
            //    if (document.getElementsByClassName("box1")[0].style.display == "none") {
            //        document.getElementsByClassName("box1")[0].style.display = "block";
            //    } else {
            //        document.getElementsByClassName("box1")[0].style.display = "none";
            //    }
            //})
            //ShowDivForDisplay();
        }
        function mouseout() {
            //document.getElementsByClassName("box1")[0].style.display = "none";
            //document.getElementsByClassName("box1")[1].style.display = "none";
            //document.getElementsByClassName("box1")[2].style.display = "none";
            var j = document.getElementsByClassName("box1").length;
            for (var i = 0; i < j; i++) {
                document.getElementsByClassName("box1")[i].style.display = "none";
            }
        }
        //function mouseover0() {
        //    document.getElementsByClassName("box1")[0].style.display = "block";
        //    document.getElementsByClassName("box1")[1].style.display = "none";
        //    document.getElementsByClassName("box1")[2].style.display = "none";
        //}
        //function mouseover1() {
        //    document.getElementsByClassName("box1")[0].style.display = "none";
        //    document.getElementsByClassName("box1")[1].style.display = "block";
        //    document.getElementsByClassName("box1")[2].style.display = "none";
        //}
        //function mouseover2() {
        //    document.getElementsByClassName("box1")[0].style.display = "none";
        //    document.getElementsByClassName("box1")[1].style.display = "none";
        //    document.getElementsByClassName("box1")[2].style.display = "block";
        //}
        function mouseover(e) {
            var j = document.getElementsByClassName("box1").length;
            for (var i = 0; i < j; i++) {
                document.getElementsByClassName("box1")[i].style.display = "none";
            }
            //alert(e);
            document.getElementsByClassName("box1")[e].style.display = "block";
        }
        //function ShowDivForDisplay() {
        //    $(".breadcrumb").each(function () {
